{
  "compilerOptions": {
    "incremental": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "lib": ["ES2022"],
    "outDir": "lib",
    "removeComments": false,
    "target": "ES2022",
    "module": "node18",
    "moduleResolution": "node16"
  },
  "exclude": ["lib/*"]
}
